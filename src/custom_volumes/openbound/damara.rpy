    # Set the scene, fade in from black
    scene bg scratchroom
    show ob_damara fiendish
    play music "music/WORST_END.wav" loop
    hide blackcover with dissolve

    # Write dialogue!
    ob_dam fiendish "ob_dam fiendish"
